
{
    "version": 2,
    "builds": [
      {
        "src": "dist/src/main.js", 
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/main.ts",
        "methods": [
            "GET",
            "DELETE", 
            "PUT", 
            "POST", 
            "PATCH"
        ]
      }
    ]
  }